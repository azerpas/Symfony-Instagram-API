            $("input[type='number']").inputSpinner({postfix: '%'});

          //date picker
            $(document).ready(function(){
                var date_input=$('input[name="dateDebut"]'); //our date input has the name "date"
                var container=$('.bootstrap-iso form').length>0 ? $('.bootstrap-iso form').parent() : "body";
                var options={
                    format: 'yyyy/mm/dd',
                    container: container,
                    todayHighlight: true,
                    autoclose: true,
                };
                date_input.datepicker(options);
            });
            $('#exampleModal').on('shown.bs.modal', function (event) {
                var tmpid = $(event.relatedTarget).attr('id');
                document.getElementById("ModalLabel").innerHTML ="de "+tmpid+"h à "+(Number(tmpid)+1)+"h" ;
                console.log(tmpid);
                document.getElementById("activer").setAttribute("onclick","activer("+tmpid+")" );
